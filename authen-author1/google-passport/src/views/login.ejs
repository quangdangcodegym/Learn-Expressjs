<h1>Sign in</h1>
<form action="/auth/login" method="post" enctype="multipart/form-data">
    <section>
        <label for="username">Username</label>
        <input id="username" name="username" type="text" autocomplete="username" required autofocus>
    </section>
    <section>
        <label for="password">Password</label>
        <input id="password" name="password" type="text" autocomplete="password" required>
    </section>
    <button type="submit">Sign in</button>
</form>
<a class="button google" href="/auth/login/google">Sign in with Google</a>